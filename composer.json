{
    "name": "wok/framework",
    "description": "The Web Operational Kit framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ©bastien ALEXANDRE",
            "email": "sebastien@graphidev.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "monolog/monolog": "^1.22",
        "league/plates": "^3.3",
        "doctrine/cache": "^1.6",
        "symfony/translation": "^3.2",
        "wok/httpmessage": "^0.4.2",
        "wok/router": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "WOK\\Scripts\\": "./src/Scripts",
            "Controllers\\": "src/Controllers",
            "Models\\": "src/Models",
        }
    },
    "scripts": {
        "post-install-cmd": [
        ],
        "post-package-install": [
        ],
        "post-update-cmd": [
        ],
        "post-package-update": [
        ],
        "post-create-project-cmd": [
            "@wok-install"
        ],
        "wok-install": [
            "WOK\\Framework\\Composer\\Scripts::install"
        ],
        "wok-update": [
            "WOK\\Framework\\Composer\\Scripts::update"
        ],
        "wok-cleanup": [
            "WOK\\Framework\\Composer\\Scripts::update"
        ]
    },
    "extra": {
        "wok-etc-dir": "etc",
        "wok-src-dir": "src",
        "wok-var-dir": "var",
        "wok-www-dir": "www"
    }
}
