{
    "name": "wok/framework",
    version: "0.1.0-dev",
    "description": "The Web Operational Kit framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ©bastien ALEXANDRE",
            "email": "sebastien@graphidev.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {},
    "autoload": {
        "psr-4": {
            "WOK\\Framework\\Composer\\": "./Composer"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@wok-install"
        ],
        "post-package-install": [
            "@wok-install"
        ],
        "post-update-cmd": [
            "@wok-update"
        ],
        "post-package-update": [
            "@wok-update"
        ],

        "post-create-project-cmd": [
            "@wok-install"
        ],
        "wok-install": [
            "WOK\\Framework\\Composer\\Scripts::install"
        ],
        "wok-update": [
            "WOK\\Framework\\Composer\\Scripts::update"
        ],
        "wok-cleanup": [
            "WOK\\Framework\\Composer\\Scripts::update"
        ]
    },
    "extra": {
        "wok-etc-dir": "etc",
        "wok-src-dir": "src",
        "wok-var-dir": "var",
        "wok-www-dir": "www"
    }
}
